{
  "name": "webssh2_client",
  "version": "2.0.0-alpha",
  "description": "A client for the webssh2 gateway",
  "author": "Bill Church",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/billchurch/webssh2_client.git",
    "directory": "."
  },
  "main": "index.js",
  "files": [
    "index.js",
    "client/index.js",
    "client/public/**/*"
  ],
  "engines": {
    "node": ">= 22"
  },
  "ignore": [
    ".gitignore"
  ],
  "scripts": {
    "start": "node index.js",
    "build": "webpack --mode production --progress --config scripts/webpack.prod.js",
    "builddev": "webpack --progress --config scripts/webpack.dev.js",
    "analyze": "webpack --json --config scripts/webpack.prod.js | webpack-bundle-size-analyzer",
    "watch": "NODE_ENV=development npm-run-all --parallel start watch:build",
    "watch:build": "webpack --watch --config scripts/webpack.dev.js",
    "publish:dry-run": "npm publish --dry-run",
    "publish:npm": "npm publish"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "debug": "^4.4.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "express": "^4.21.2",
    "html-webpack-plugin": "^5.6.3",
    "jsmasker": "^1.4.0",
    "mini-css-extract-plugin": "^2.9.2",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "purecss": "^3.0.0",
    "socket.io-client": "4.8.1",
    "terser-webpack-plugin": "^5.3.11",
    "uglify-js": "^3.19.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^6.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard": {
    "ignore": [
      "client/public/**/*"
    ]
  }
}
