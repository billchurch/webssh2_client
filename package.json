{
  "name": "webssh2_client",
  "version": "0.2.13",
  "ignore": [
    ".gitignore"
  ],
  "description": "A client for the webssh2 gateway",
  "main": "index.js",
  "files": [
    "index.js",
    "client/index.js",
    "client/public/**/*"
  ],
  "engines": {
    "node": ">= 18"
  },
  "scripts": {
    "start": "node index.js",
    "build": "webpack --mode production --progress --config scripts/webpack.prod.js",
    "builddev": "webpack --progress --config scripts/webpack.dev.js",
    "analyze": "webpack --json --config scripts/webpack.prod.js | webpack-bundle-size-analyzer",
    "watch": "npm-run-all --parallel start watch:build",
    "watch:build": "webpack --watch --config scripts/webpack.dev.js",
    "prepublishOnly": "npm run build"
  },
  "author": "",
  "license": "SEE LICENSE IN FILE - LICENSE",
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "debug": "^4.3.5",
    "express": "^4.19.2",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.0",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "socket.io-client": "^2.2.0",
    "terser-webpack-plugin": "^5.3.10",
    "uglify-js": "^3.19.0",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^6.0.1"
  }
}
