{
  "name": "webssh2_client",
  "version": "0.2.17",
  "ignore": [
    ".gitignore"
  ],
  "description": "A client for the webssh2 gateway",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/billchurch/webssh2_client.git"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "client/index.js",
    "client/public/**/*"
  ],
  "engines": {
    "node": ">= 18"
  },
  "scripts": {
    "start": "node index.js",
    "build": "webpack --mode production --progress --config scripts/webpack.prod.js",
    "builddev": "webpack --progress --config scripts/webpack.dev.js",
    "analyze": "webpack --json --config scripts/webpack.prod.js | webpack-bundle-size-analyzer",
    "watch": "NODE_ENV=development npm-run-all --parallel start watch:build",
    "watch:build": "webpack --watch --config scripts/webpack.dev.js",
    "prepublishOnly": "npm run build",
    "version": "npm run build && conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md client/public/*",
    "release": "standard-version -a -s",
    "publish:dry-run": "npm publish --dry-run",
    "publish:npm": "npm publish",
    "release:major": "npm run release -- --release-as major",
    "release:minor": "npm run release -- --release-as minor",
    "release:patch": "npm run release -- --release-as patch"
  },
  "standard-version": {
    "scripts": {
      "postbump": "npm run build",
      "precommit": "git add client/public/*"
    }
  },
  "author": "",
  "license": "SEE LICENSE IN FILE - LICENSE",
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "clean-webpack-plugin": "^4.0.0",
    "conventional-changelog-cli": "^5.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "express": "^4.19.2",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.0",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "socket.io-client": "2.2.0",
    "standard-version": "^9.5.0",
    "terser-webpack-plugin": "^5.3.10",
    "uglify-js": "^3.19.0",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "debug": "^4.3.6",
    "purecss": "^3.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard": {
    "ignore": [
      "client/public/**/*"
    ]
  }
}
